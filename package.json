{
  "name": "ReactBoilerplate",
  "version": "0.0.1",
  "author": "mfma",
  "description": "create a react boilerplate",
  "keywords": [
    "react",
    "redux",
    "react-router",
    "webpack",
    "boilerplate"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development node webpack/webpack.dev.js --hot",
    "build": "cross-env NODE_ENV=production  webpack --config webpack/webpack.prod.js"
  },
  "license": "MIT",
  "babel": {
    "plugins": [
      "transform-object-rest-spread",
      "transform-es2015-arrow-functions"
    ],
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "mirrorx": "^0.2.6"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "cross-env": "^5.0.5",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^1.3.1",
    "sass-loader": "^6.0.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mfma/ReactBoilerplate.git"
  }
}
